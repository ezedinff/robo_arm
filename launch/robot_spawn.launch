<launch>
  <!--Include description and control launch files-->
  <include file="$(find robo_arm)/launch/robot_description.xml"/>
  <include file="$(find robo_arm)/launch/robot_control.xml"/>
 <param name="robot_description" command="$(find xacro)/xacro.py '$(find robo_arm)/urdf/robo_arm.urdf.xacro'" />
  <!--Launch a gazebo world-->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">

  </include>
 <!--spawn a robot in gazebo world -->
  <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
  args="-urdf -param robot_description -model robo_arm -x 0 -y 0 -z 0 -R 0 -P 0 -Y 0 "/>

</launch>
